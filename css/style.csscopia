.container {
  width: 80%;
  margin: 0 auto;
  max-width: 1200px;
}
/* Estilo básico para inicio de sesion*/
.sesion {
  font-family: Arial, sans-serif;
  background-color: #c89c9c;
  margin: 0;
  padding: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  height: 100vh;
}

/* Contenedor del formulario */
form {
  position: relative;
  top: -200px;
  margin-left: 24cm;
  background-color: #d9ccc5;
  padding: 10px;
  border-radius: 8px;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
  width: 300px;
  text-align: center;
}

/* Estilo para los encabezados */
.usuario {
  font-size: 1cm;
  font-family: "Lucida Sans", "Lucida Sans Regular", "Lucida Grande",
    "Lucida Sans Unicode", Geneva, Verdana, sans-serif;
  margin-bottom: 20px;
  text-align: center;
  color: white;
  padding: 250px;
}

/* Estilo para las etiquetas de los campos */
label {
  font-size: 14px;
  color: #555;
  text-align: left;
  display: block;
  margin-bottom: 45px;
}

/* Estilo para los campos de texto (input) */
input[type="text"],
input[type="password"] {
  width: 100%;
  padding: 10px;
  margin-bottom: 30px;
  border: 1px solid #ccc;
  border-radius: 4px;
  font-size: 16px;
  box-sizing: border-box;
}

/* Estilo para el botón */
button {
  background-color: #8d8d54;
  color: white;
  border: none;
  padding: 10px 20px;
  font-size: 16px;
  cursor: pointer;
  border-radius: 4px;
  width: 100%;
}
